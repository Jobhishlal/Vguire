        /* Blocked User Page Styles */
.blocked-wrapper {
    display: flex;
    justify-content: center;
    align-items: center;
    height: 100vh;
    background: white
}

.blocked-container {
    display: flex;
    flex-direction: column;
    align-items: center;
    background: white;
    border-radius: 15px;
    padding: 30px;
    box-shadow: 0px 4px 15px rgba(0, 0, 0, 0.1);
    width: 70%;
    max-width: 600px;
    text-align: center;
}

.hero-section {
    position: relative;
    width: 100%;
    height: 200px;
    background: url('/path/to/your/blocked-background.jpg') center center no-repeat;
    background-size: cover;
    border-radius: 15px;
}

.blocked-track {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.4);
}

.blocked-user {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    font-size: 40px;
    color: white;
}

.blocked-icons {
    position: absolute;
    bottom: 10%;
    left: 50%;
    transform: translateX(-50%);
    font-size: 30px;
    color: white;
}

.blocked-card {
    background: #f8f9fa;
    border-radius: 10px;
    padding: 20px;
    width: 100%;
}

.blocked-badge {
    width: 40px;
    height: 40px;
    margin: 0 auto;
    background: #ff6b6b;
    display: flex;
    justify-content: center;
    align-items: center;
    border-radius: 50%;
    color: white;
    margin-bottom: 20px;
}

.blocked-text {
    font-size: 30px;
    font-weight: bold;
    color: #ff6b6b;
    margin-bottom: 20px;
}

.text-center {
    color: #6c757d;
}

.blocked-action-buttons {
    display: flex;
    justify-content: center;
    gap: 20px;
}

.blocked-action-buttons .btn-contact-support,
.blocked-action-buttons .btn-login {
    padding: 10px 20px;
    background-color: #ff6b6b;
    color: white;
    border-radius: 5px;
    text-decoration: none;
    font-size: 16px;
}

.blocked-action-buttons .btn-contact-support:hover,
.blocked-action-buttons .btn-login:hover {
    background-color: #e03e3e;
}

        
